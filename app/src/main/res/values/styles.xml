<resources>
    <!-- Base application theme. -->
    <style name="AppTheme" parent="Theme.AppCompat.Light.DarkActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/colorPrimary</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
        <item name="colorAccent">@color/colorAccent</item>
    </style>

    <!-- Custom default theme colors -->
    <style name="CustomAppTheme" parent="AppTheme">
        <item name="titleTextColor">@color/titleTextColor</item>
        <item name="iconTint">@color/iconTint</item>
    </style>

    <style name="BlackOverflow" parent="Widget.AppCompat.Light.ActionButton.Overflow">
        <item name="android:src">@drawable/ic_baseline_more_vert_black_24</item>
    </style>
</resources>